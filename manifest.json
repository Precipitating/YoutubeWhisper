{
  "description": "Utilizes whisper to transcribe videos",
  "manifest_version": 2,
  "name": "YoutubeWhisper",
  "version": "1.0",
  "browser_action": {
    "default_popup": "src/app.html"
  },

  "web_accessible_resources": ["src/audiopreprocessor.js"],

  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/watch*"],
      "js": ["build/app.js"]
    }
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
